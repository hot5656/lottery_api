<!DOCTYPE html>
<html lang="en">
<head>
	<%- include('./template/head') %>
	<title>Document</title>
</head>
<body>
	<%- include('./template/navbar') %>
	<div class="container">
		<table class="table table-hover">
				<thead>
					<tr class="row">
						<th class="col-2">username</th>
						<th class="col-2">nickname</th>
						<th class="col-1">user_level</th>
						<th class="col-7">password</th>
					</tr>
				</thead>
				<tbody>
					<% users.forEach( function(user) { %>
					<tr class="row">
						<td class="col-2"><%= user.username %></td>
						<td class="col-2"><%= user.nickname %></td>
						<td class="col-1"><%= user.user_level %></td>
						<td class="col-7"><%= user.password %></td>
					</tr>
					<% }) %>
				</tbody>
			</table>
	</div>
</body>
</html>